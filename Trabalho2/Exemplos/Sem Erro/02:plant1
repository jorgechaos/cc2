// Plant-like structure generated by bracketed OL-systems. This L-system is edge-rewriting.
alphabet:	'F':FORWARD(2),
			'-':ROTATE(22.5),
			'+':ROTATE(-22.5),
			'[':PUSH,
			']':POP

axiom:		'F'

rules:		'F' -> 'F' 'F'   '-' '[' '-' 'F'(0.75*v) '+' 'F'(0.75*v) '+' 'F'(0.75*v) ']'   '+' '[' '+' 'F'(0.75*v) '-' 'F'(0.75*v) '-' 'F'(0.75*v) ']'
